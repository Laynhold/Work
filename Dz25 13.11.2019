public class dz25 {
	public static void main(String args[]) {
		Car f1 = new Car();
		f1.start();
		f1.drive(30);
		f1.stop();
	}
}
class Car {
	public void start() {
		System.out.println("Старт.");
	}
	
	public void drive(int howlong){
		int distance = howlong*60;
		System.out.println ("Проехали: " + distance + ".");
		
	}
	public void stop() {
		System.out.println("Финиш.");
	}
}
