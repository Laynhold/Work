public class dz29 {
	public static void main(String args[]){
		int k, o, t, a, b, c;
		for (k = 1; k < 10; k++){
			for (o = 0; o < 10; o++){
				for (t = 0; t < 10; t++){
					a = (100 * k) + (10 * t) + o;
					b = (100 * k) + (10 * o) + t;
					c = (100 * t) + (10 * o) + k;
					if (a + b == c){
						System.out.println(a + " + " + b + " = " + c);
					}
				}
			}
			
			
		}
		
		
	}

}
