public class massiv1 {
	public static void main(String args[]){
		int [] mas = new int[10];
		int maxNumber = 10;
		int maxIndex = 0;
		int minIndex = 0;
		for (int i = 0; i < mas.length; i++){
			mas[i] = (int)(Math.random()*21)+10;
			if (maxNumber <= mas[i]){
				maxNumber = mas[i];
				maxIndex = i;
				minIndex = i;
		System.out.println("Максимальное число: " + maxIndex);
		System.out.println("Минимальное число: " + minIndex);
			}
		}
		
	}
}
