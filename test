public class dz28 {
	public static void main(String args[]){
		Person david = new Person ("Давид", 17, "45 09 399123", "123-456-789 00");
		david.displayName();
		david.displayAge();
		//david.displayPasp();
		//david.displaySnils();
		System.out.println(david.name);
		System.out.println(david.age);
		//System.out.println(david.pasp);
		//System.out.println(david.snils);
		Person vetal = new Person ("Виталий", 18, "44 99 659113", "623-609-991 00");
		vetal.displayName();
		vetal.displayAge();
		//vetal.displayPasp();
		//vetal.displaySnils();
		System.out.println(vetal.name);
		System.out.println(vetal.age);
		//System.out.println(vetal.pasp);
		//System.out.println(vetal.snils);
	}
}
class Person {
	
	String name;
	protected int age;
	private String pasp;
	private String snils;
	
	public Person(String name, int age, String pasp, String snils){
		this.name = name;
		this.age = age;
		this.pasp = pasp;
		this.snils = snils;
	}
	public void displayName(){
		System.out.println("Имя: " + name);
	}
	protected void displayAge(){
		System.out.println("Возраст: " + age);
	}
	private void displayPasp(){
		System.out.println("Номер паспорта: " + pasp);
	}
	private void displaySnils(){
		System.out.println("Номер снилса: " + snils);
	}
}
